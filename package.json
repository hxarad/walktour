{
  "name": "walktour",
  "version": "5.2.0",
  "description": "Guided tour/walkthrough component for react",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "parcel demo/index.html --open",
